{
  "dbt-formatter.upper": true,
  "dbt-formatter.lowerWords": true,
  "dbt-formatter.camelCase": true,
  "files.associations": {
    "*.yaml": "jinja-yaml",
    "*.yml": "jinja-yaml",
    "*.sql": "jinja-sql",
    "*.md": "jinja-md",
    "**/target/compiled/**/*.sql": "oraclesql",
  },
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "files.trimTrailingWhitespace": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
}