{
  "name": "DBT Dev Container",
  "dockerFile": "Dockerfile",
  "forwardPorts": [
    5432
  ],
  "postCreateCommand": "dbt debug",
  "customizations": {
    "vscode": {
            "files.associations": {
        "*.yaml": "jinja-yaml",
        "*.yml": "jinja-yaml",
        "*.sql": "jinja-sql",
        "*.md": "jinja-md"
      }
    },
    "extensions": [
            "innoverio.vscode-dbt-power-user"
    ]
  }
}